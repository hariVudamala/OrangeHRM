package com.ex;

public class EX2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		for(int i=1;i<=10;i=i+2)
		{
			for(int j=0;j<i;j=j+2)
			{
				System.out.print("*");
			}
			System.out.println("");
		}
		for(int n=1;n<6;n++)
		{
			for(int m=6;m>n;m--)
			{
				System.out.print("*");
			}
			System.out.println("");
		}
		
	}

}
